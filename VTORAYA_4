#include <iostream>
using namespace std;

int main() {
    int num; // Определение переменной целого типа
    cout << "Введите число: ";
    cin >> num; // Ввод числа с клавиатуры

    // Деление числа на 128 с использованием поразрядной операции сдвига вправо
    num = num >> 7; // Сдвиг вправо на 7 битов (эквивалентно делению на 128)

    // Вывод результата на монитор
    cout << "Результат деления на 128: " << num << endl;

    return 0;
}
